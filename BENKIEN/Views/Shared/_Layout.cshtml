        <link rel="stylesheet" href="~/css/bootstrap/bootstrap.min.css">
    </noscript>

    <!--================= Bootstrap Icons Style -->
    <link type="text/css" rel="preload" href="/css/bs-icons/bootstrap-icons.css" as="style"
          onload="this.onload=null;this.rel='stylesheet'">
